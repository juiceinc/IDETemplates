<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="juice">
  <template name="juicecommand" value="private static function $NAME$($ARGS$):$RETURN_TYPE$ {&#10; $END$ &#10;}" description="Simple command" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN_TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_COMMENT" value="false" />
      <option name="JAVA_STRING" value="false" />
      <option name="COMPLETION" value="false" />
      <option name="HTML" value="false" />
      <option name="XML" value="false" />
      <option name="JSP" value="false" />
      <option name="SQL" value="false" />
      <option name="CSS" value="false" />
      <option name="GROOVY" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="ACTION_SCRIPT" value="true" />
      <option name="GSP" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
</templateSet>

